<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant value="false" name="struts.enable.DynamicMethodInvocation" />
	<constant value="true" name="struts.devMode" />
	<package name="helloworld" extends="struts-default">
		<action name="LoginAction" class="helloworld.LoginAction">
			<result name="success">/WEB-INF/jsp/hellworld/welcome.jsp</result>
			<result name="error">/WEB-INF/jsp/hellworld/error.jsp</result>
			<result name="input"></result>
		</action>
	</package>
</struts>	